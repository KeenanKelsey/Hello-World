import random

def random_walk(n):
    ''' Return coordinates for 'n' blocks random walks'''
x, y = 0, 0
for i in range(n):
    (dx, dy) = random.choice([(0, 1), (0, -1), (1, 0), (-1, 0)])
    x += dx
    y += dy

    return (x, y)

for i in range(25):
    walk = random_walk_2(10)
    print(walk, "Distance from home = ",
          abs([0]) + abs([1]))
